{
  "name": "huasengheng-tracker-extension",
  "version": "1.0.0",
  "description": "For tracking gold price profits and helping in making gold trading decisions.",
  "scripts": {
    "build": "npm run clean && parcel build manifest.json --config @parcel/config-webextension && npm run cp-popup",
    "start": "parcel watch manifest.json --host localhost --config @parcel/config-webextension",
    "clean": "rm -rf dist .parcel-cache",
    "cp-popup": "cp /Users/thanaditbuthong/Applications/ChromeExtension/huasengheng-tracker-extension/src/popups/popup.html /Users/thanaditbuthong/Applications/ChromeExtension/huasengheng-tracker-extension/dist"
  },
  "keywords": [
    "huasengheng",
    "gold"
  ],
  "author": "Thanadit Buthong",
  "license": "ISC",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.12.0",
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10"
  }
}
